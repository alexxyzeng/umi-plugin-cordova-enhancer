<?xml version='1.0' encoding='utf-8'?>
<widget defaultLocale="zh_CN" defaultlocale="zh_CN" id="com.dfocus.fmapp" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>
        fmapp
    </name>
    <description>
        fmapp
    </description>
    <author email="xxx@df.com" href="#">
        xxx
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="target-device" value="handset" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="SwiftVersion" value="5" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="Orientation" value="portrait" />
    <platform name="android">
        <allow-intent href="market:*" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:debuggable="true" android:usesCleartextTraffic="true" />
        </edit-config>
        <icon density="xxhdpi" src="res/icon/android/icon-144-xxhdpi.png" />
        <icon density="xxxhdpi" src="res/icon/android/icon-192-xxxhdpi.png" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
        <splash density="land-hdpi" src="res/splash/android/splash-land-hdpi.png" />
        <splash density="land-ldpi" src="res/splash/android/splash-land-ldpi.png" />
        <splash density="land-mdpi" src="res/splash/android/splash-land-mdpi.png" />
        <splash density="land-xhdpi" src="res/splash/android/splash-land-xhdpi.png" />
        <splash density="port-hdpi" src="res/splash/android/splash-port-hdpi.png" />
        <splash density="port-ldpi" src="res/splash/android/splash-port-ldpi.png" />
        <splash density="port-mdpi" src="res/splash/android/splash-port-mdpi.png" />
        <splash density="port-xhdpi" src="res/splash/android/splash-port-xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="1024" src="res/icon/ios/icon-1024.png" width="1024" />
        <icon height="20" src="res/icon/ios/icon-20.png" width="20" />
        <icon height="29" src="res/icon/ios/icon-29.png" width="29" />
        <icon height="58" src="res/icon/ios/icon-29@2x.png" width="58" />
        <icon height="87" src="res/icon/ios/icon-29@3x.png" width="87" />
        <icon height="40" src="res/icon/ios/icon-40.png" width="40" />
        <icon height="80" src="res/icon/ios/icon-40@2x.png" width="80" />
        <icon height="50" src="res/icon/ios/icon-50.png" width="50" />
        <icon height="100" src="res/icon/ios/icon-50@2x.png" width="100" />
        <icon height="57" src="res/icon/ios/icon-57@1x.png" width="57" />
        <icon height="114" src="res/icon/ios/icon-57@2x.png" width="114" />
        <icon height="60" src="res/icon/ios/icon-60.png" width="60" />
        <icon height="120" src="res/icon/ios/icon-60@2x.png" width="120" />
        <icon height="180" src="res/icon/ios/icon-60@3x.png" width="180" />
        <icon height="72" src="res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="res/icon/ios/icon-72@2x.png" width="144" />
        <icon height="76" src="res/icon/ios/icon-76.png" width="76" />
        <icon height="152" src="res/icon/ios/icon-76@2x.png" width="152" />
        <icon height="167" src="res/icon/ios/icon-83.5@2x.png" width="167" />
        <splash height="2436" src="res/splash/ios/Default-2436h.png" width="1125" />
        <splash height="1136" src="res/splash/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/splash/ios/Default-667h@2x~iphone.png" width="750" />
        <splash height="2208" src="res/splash/ios/Default-736h@3x~iphone.png" width="1242" />
        <splash height="1242" src="res/splash/ios/Default-Landscape-736h@3x~iphone.png" width="2208" />
        <splash height="1536" src="res/splash/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="768" src="res/splash/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" src="res/splash/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="1024" src="res/splash/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="960" src="res/splash/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" src="res/splash/ios/Default~iphone.png" width="320" />
        <config-file parent="NSMicrophoneUsageDescription " platform="ios" target="*-Info.plist">
            <string>
                xxxx我们要使用您的相机
            </string>
        </config-file>
        <config-file parent="NSPhotoLibraryUsageDescription" platform="ios" target="*-Info.plist">
            <string>
                use photo
            </string>
        </config-file>
        <config-file parent="NSLocationWhenInUseUsageDescription" platform="ios" target="*-Info.plist">
            <string>
                use location
            </string>
        </config-file>
        <edit-config file="*-Info.plist" mode="merge" target="NSMicrophoneUsageDescription">
            <string>
                use microphone
            </string>
        </edit-config>
    </platform>
    <plugin name="cordova-plugin-device" spec="~2.0.3" />
    <plugin name="cordova-plugin-file" spec="~6.0.2" />
</widget>
