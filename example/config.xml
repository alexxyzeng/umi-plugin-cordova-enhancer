        <splash src="res/splash/ios/Default-2436h.png" height="2436" width="1125"></splash>
        <splash src="res/splash/ios/Default-568h@2x~iphone.png" height="1136" width="640"></splash>
        <splash src="res/splash/ios/Default-667h@2x~iphone.png" height="1334" width="750"></splash>
        <splash src="res/splash/ios/Default-736h@3x~iphone.png" height="2208" width="1242"></splash>
        <splash src="res/splash/ios/Default-Landscape-736h@3x~iphone.png" height="1242" width="2208"></splash>
        <splash src="res/splash/ios/Default-Landscape@2x~ipad.png" height="1536" width="2048"></splash>
        <splash src="res/splash/ios/Default-Landscape~ipad.png" height="768" width="1024"></splash>
        <splash src="res/splash/ios/Default-Portrait@2x~ipad.png" height="2048" width="1536"></splash>
